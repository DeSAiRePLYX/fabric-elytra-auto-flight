{
  "schemaVersion": 1,
  "id": "elytraautoflight",
  "version": "${version}",

  "name": "Elytra Auto Flight",
  "description": "This mod will fly your elytra wings for you, and will gain altitude without the use of fireworks. ",
  "authors": [
    "LouisLOurson"
  ],
  "contact": {
    "homepage": "https://github.com/simonlourson/fabric-elytra-auto-flight",
    "sources": "https://github.com/simonlourson/fabric-elytra-auto-flight"
  },

  "license": "CC0-1.0",
  "icon": "assets/elytraautoflight/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.elytraautoflight.ElytraAutoFlight"
    ]
  },
  "mixins": [
    "elytraautoflight.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
  },
  "suggests": {
    "flamingo": "*"
  }
}
